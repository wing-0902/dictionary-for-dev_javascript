{
  "name": "dictionary4.dev_javascript",
  "type": "module",
  "version": "0.0.1",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/wing-0902/dictionary4.dev_javascript.git"
  },
  "author": {
    "name": "Wing",
    "email": "wing@me.wing.osaka",
    "url": "https://wing.osaka/"
  },
  "scripts": {
    "license:listup": "node generate-license.mjs",
    "license:generate": "license-checker --json --production --packages ./node_modules/.pnpm --out src/data/license-data.json",
    "dev": "pnpm license:generate && astro dev",
    "build": "pnpm license:generate && astro build",
    "pagefind:full": "pagefind --site dist/content --output-path dist/content_search",
    "pagefind:title_only": "pagefind --site dist/content --root-selector 'h1' --output-path dist/title_only_search",
    "pagefind:code_only": "pagefind --site dist/content --root-selector 'h1, code' --output-path dist/code_only_search",
    "postbuild": "pnpm pagefind:full && pnpm pagefind:title_only && pnpm pagefind:code_only",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@alpinejs/mask": "^3.15.3",
    "@alpinejs/persist": "^3.15.2",
    "@astrojs/alpinejs": "^0.4.9",
    "@astrojs/cloudflare": "^12.6.12",
    "@astrojs/markdoc": "^0.15.10",
    "@astrojs/mdx": "^4.3.13",
    "@astrojs/sitemap": "^3.6.0",
    "@astrojs/svelte": "^7.2.3",
    "@astrojs/vue": "^5.1.3",
    "@codemirror/lang-javascript": "^6.2.4",
    "@shikijs/transformers": "^3.19.0",
    "alpinejs": "^3.15.2",
    "astro": "^5.16.5",
    "html2canvas": "^1.4.1",
    "license-checker": "^25.0.1",
    "localforage": "^1.10.0",
    "pagefind": "^1.4.0",
    "sass": "^1.94.0",
    "svelte": "^5.43.6",
    "svelte-codemirror-editor": "^2.1.0",
    "svelte-turnstile": "^0.11.0",
    "typescript": "^5.9.3",
    "uqr": "^0.1.2",
    "uuid": "^13.0.0",
    "vue": "^3.5.24"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251209.0",
    "@types/alpinejs": "^3.13.11",
    "@types/alpinejs__mask": "^3.13.4",
    "@types/alpinejs__persist": "^3.13.4",
    "sharp": "^0.34.5"
  }
}
